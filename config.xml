<?xml version='1.02' encoding='utf-8'?>
<widget id="com.soulrebelent.rediyo" version="1.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rediyo</name>
    <description>
        Music Streaming and Download Application
    </description>
    <author email="info@apptitudetech.net" href="http://apptitudetech.net">
        Apptitude Tech Systems
    </author>
    <content src="start.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
